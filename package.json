{
  "name": "agora",
  "version": "1.0.0",
  "description": "Bring all your data and information sources to one place!",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "test": "mocha --slow 25 --exit \"./{,!(node_modules)/**/}*.spec.js\"",
