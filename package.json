    "mysql": "2.18.1",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "^6.8.0",
    "nyc": "^15.0.1",
    "mocha": "^7.1.1",
    "nodemon": "2.0.3",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "sinon-express-mock": "^2.2.1"
  }
